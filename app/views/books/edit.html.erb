<div>
  <%= flash[:notice] %>
</div>

<h2> Editing Book</h2>

<!-- form_with部分 -->
<%= form_with model: @book do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>
	
  <h4>Body</h4>
  <%= f.text_area :body %>
	
  <h4><%= f.submit 'Update Book' %></h4>
<% end %>

| <%= link_to "Back", books_path %>